<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.adndiginet.terbb">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/oboshor_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/oboshor_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.TERBB.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".UI.NoticeBoardActivity"
            android:exported="false" />
        <activity
            android:name=".UI.CommunicationActivity"
            android:exported="false" />
        <activity
            android:name=".UI.AboutUsActivity"
            android:exported="false" />
        <activity
            android:name=".UI.ImportantLinkActivity"
            android:exported="false" />
        <activity
            android:name=".UI.MemberDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".UI.ReviewActivity"
            android:exported="false" />
        <activity
            android:name=".UI.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".UI.FaqActivity"
            android:exported="false" />
        <activity
            android:name=".UI.WebviewActivity"
            android:exported="false" />
        <activity
            android:name=".UI.CurrentSituationActivity"
            android:exported="false" />
        <activity
            android:name=".UI.OtpActivity"
            android:exported="false" />
        <activity
            android:name=".UI.IndexSearchActivity"
            android:exported="false" />
        <activity
            android:name=".UI.FormActivity"
            android:exported="false" />
        <activity
            android:name=".UI.MainActivity"
            android:exported="true" />
    </application>

</manifest>